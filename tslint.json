{
	"defaultSeverity": "error",
	"extends": [
      "tslint:recommended",
      "tslint-eslint-rules"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [
      true,
      "never"
    ],
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "trailing-comma": false,
    "max-line-length": false,
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "member-ordering": false,
    "no-bitwise": false,
    "max-classes-per-file": false,
    "only-arrow-functions": false,
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "ordered-imports": false,
    "no-unused-variable": true,
    "variable-name": [
      true,
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "object-curly-spacing": [
      true,
      "always"
    ]
	},
	"rulesDirectory": []
}
