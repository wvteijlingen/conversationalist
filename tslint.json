{
	"defaultSeverity": "error",
	"extends": [
			"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [true, "never"],
		"interface-name": false,
		"member-access": [true, "no-public"],
		"object-literal-sort-keys": false,
		"arrow-parens": [true, "ban-single-arg-parens"],
		"trailing-comma": false,
		"max-line-length": false,
		"whitespace": [true,
			"check-decl",
			"check-operator",
			"check-module",
			"check-separator",
			"check-rest-spread",
			"check-type",
			"check-typecast",
			"check-type-operator",
			"check-preblock",
			"check-postbrace"
		],
		"member-ordering": false,
		"no-bitwise": false
	},
	"rulesDirectory": []
}
